digraph StateMachine {
    "Closed" [shape=circle];
    "Locked" [shape=doublecircle, style=filled, fillcolor=lightblue];
    "Open" [shape=circle];
    "Closed" -> "Locked" [label="LockDoor"];
    "Closed" -> "Open" [label="OpenDoor"];
    "Locked" -> "Closed" [label="UnlockDoor"];
    "Open" -> "Closed" [label="CloseDoor"];
}